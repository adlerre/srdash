    "scripts": {
        "ng": "ng",
        "start": "ng serve --host 0.0.0.0",
        "build": "ng build --configuration production --build-optimizer --aot=true --stats-json",
        "analyzer": "webpack-bundle-analyzer target/classes/META-INF/resources/stats.json",
